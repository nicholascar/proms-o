@prefix promsrole: <http://promsns.org/def/promsrole/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://promsns.org/def/promsrole>
  rdf:type owl:Ontology ;
  dc:source "ISO 19115-1:2014"@en ;
  dct:contributor <http://people.csiro.au/Nicholas-Car> ;
  dct:created "2016-10-01"^^xsd:date ;
  dct:license <http://registry.it.csiro.au/licence/cc-by-4.0> ;
  dct:source <http://standards.iso.org/iso/19115/resources/Codelist/gml/CI_RoleCode.xml> ;
  rdfs:comment "An initial set of Agent (Person) role definitions from ISO 19115-1:2014 CI_RoleCode presented both as an OWL ontology and a SKOS vocabulary."@en ;
  rdfs:comment "List of roles that agents play in relation to datasets and their change events"@en ;
  owl:imports <http://communications.data.gov.au/def/dataset> ;
  prov:qualifiedAttribution [
      rdf:type prov:Attribution ;
      prov:agent <http://people.csiro.au/Nicholas-Car> ;
      prov:hadRole aurole:Processor ;
    ] ;
  prov:qualifiedAttribution [
      rdf:type prov:Attribution ;
      prov:agent <http://people.csiro.au/Simon-Cox> ;
      prov:hadRole aurole:Processor ;
    ] ;	
  prov:wasDerivedFrom <http://standards.iso.org/iso/19115/resources/Codelist/cat/codelists.html> ;
  prov:wasDerivedFrom <https://raw.githubusercontent.com/AGLDWG/TR/master/lists/aurole.ttl> ;
.
promsrole:Author
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party who authored the resource"@en ;
  rdfs:label "author"@en ;
  skos:definition "party who authored the resource"@en ;
.
promsrole:Coauthor
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party who jointly authors the resource"@en ;
  rdfs:label "co-author"@en ;
  skos:definition "party who jointly authors the resource"@en ;
.
promsrole:Collaborator
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party who assists with the generation of the resource other than the principal investigator"@en ;
  rdfs:label "collaborator"@en ;
  skos:definition "party who assists with the generation of the resource other than the principal investigator"@en ;
.
promsrole:Contributor
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party contributing to the resource"@en ;
  rdfs:label "contributor"@en ;
  skos:definition "party contributing to the resource"@en ;
.
promsrole:Custodian
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party that accepts accountability and responsibility for the resource and ensures appropriate care and maintenance of the resource"@en ;
  rdfs:label "custodian"@en ;
  skos:definition "party that accepts accountability and responsibility for the resource and ensures appropriate care and maintenance of the resource"@en ;
.
promsrole:Distributor
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party who distributes the resource"@en ;
  rdfs:label "distributor"@en ;
  skos:definition "party who distributes the resource"@en ;
.
promsrole:Editor
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party who reviewed or modified the resource to improve the content"@en ;
  rdfs:label "editor"@en ;
  skos:definition "party who reviewed or modified the resource to improve the content"@en ;
.
promsrole:Funder
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party providing monetary support for the resource"@en ;
  rdfs:label "funder"@en ;
  skos:definition "party providing monetary support for the resource"@en ;
.
promsrole:Mediator
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "a class of entity that mediates access to the resource and for whom the resource is intended or useful"@en ;
  rdfs:label "mediator"@en ;
  skos:definition "a class of entity that mediates access to the resource and for whom the resource is intended or useful"@en ;
.
promsrole:Originator
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party who created the resource"@en ;
  rdfs:label "originator"@en ;
  skos:definition "party who created the resourceuseful"@en ;
.
promsrole:Owner
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party that owns the resource"@en ;
  rdfs:label "owner"@en ;
  skos:definition "party that owns the resource"@en ;
.
promsrole:PointOfContact
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party who can be contacted for acquiring knowledge about or acquisition of the resource"@en ;
  rdfs:label "point of contact"@en ;
  skos:definition "party who can be contacted for acquiring knowledge about or acquisition of the resource"@en ;
  skos:exactMatch <http://www.w3.org/ns/dcat#contactPoint> ;
.
promsrole:PrincipalInvestigator
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "key party responsible for gathering information and conducting research"@en ;
  rdfs:label "principal investigator"@en ;
  skos:definition "key party responsible for gathering information and conducting research"@en ;
.
promsrole:Processor
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party who has processed the data in a manner such that the resource has been modified"@en ;
  rdfs:label "processor"@en ;
  skos:definition "party who has processed the data in a manner such that the resource has been modified"@en ;
.
promsrole:Publisher
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party who published the resource"@en ;
  rdfs:label "publisher"@en ;
  skos:definition "party who published the resource"@en ;
  skos:exactMatch dct:publisher ;
.
promsrole:ResourceProvider
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party that supplies the resource"@en ;
  rdfs:label "resource provider"@en ;
  skos:definition "party that supplies the resource"@en ;
.
promsrole:RightsHolder
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party owning or managing rights over the resource"@en ;
  rdfs:label "rights holder"@en ;
  skos:definition "party owning or managing rights over the resource"@en ;
.
promsrole:Sponsor
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party who speaks for the resource"@en ;
  rdfs:label "sponsor"@en ;
  skos:definition "party who speaks for the resource"@en ;
.
promsrole:Stakeholder
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party who has an interest in the resource or the use of the resource"@en ;
  rdfs:label "stakeholder"@en ;
  skos:definition "party who has an interest in the resource or the use of the resource"@en ;
.
promsrole:User
  rdf:type prov:Role , skos:Concept ;
  rdfs:comment "party who uses the resource"@en ;
  rdfs:label "user"@en ;
  skos:definition "party who uses the resource"@en ;
.